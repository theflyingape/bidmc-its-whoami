{
  "name": "Who Am I",
  "short_name": "Who Am I",
  "version": "1.3.0",
  "content_security_policy": "script-src 'self' *.bidmc.harvard.edu;",
  "permissions": [
    "enterprise.deviceAttributes",
    "fileSystem",
    "https://*.bidmc.harvard.edu/",
    "https://*.bidmc.org/",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.network",
    "system.storage"
  ],
  "minimum_chrome_version": "69",
  "manifest_version": 2,
  "description": "BIDMC ITS: managed Chrome OS support",
  "app": {
    "background": {
      "scripts": [ "main.js" ]
    }
  },
  "storage": {
    "managed_schema": "schema.json"
  },
  "author": "Robert Hurst <rhurst@bidmc.harvard.edu>",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon.png"
  }
}
